
    <br/>
    <div class="row">     
        <div class="col-lg-7">
            <div class="panel panel-primary">
                  
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="glyphicon glyphicon-th-list"></i> Logiciel</h3>
                </div>
                <div class="panel-body">
                    {{dump(app.request.scheme ~'://' ~ app.request.httpHost ~ asset('bootstrap/css/bootstrap.min.css'))}}
                    <div class="col-lg-12">
                        <div class="panel panel-info">
                            {% if logiciel != null %}
                            <div class="panel-heading">
                               
                                <h3 class="panel-title"><i class="fa fa-laptop"></i> Liste logiciel</h3>
                            </div>
                                
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>Nom du logiciel</th>
                                        <th>Nom editeur</th>
                                        <th> version</th>
                                        <th> licence </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {% for logiciels in logiciel %}
                                       
                                        <tr>
                                            <td>{{ logiciels.nomLog }}</td>
                                            <td>{{ logiciels.nomEditeur }}</td>
                                            <td>{{ logiciels.versionLog}}</td>
                                           <td>{{  logiciels.licence}}</td>
                                        </tr>
                                   
                                    {% endfor %}
                                </tbody>
                            </table>
                                {% endif %}
                        </div>
                    </div>
                  
                </div>
            </div>
        </div>
    </div>
